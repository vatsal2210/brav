<body ng-controller="indexCtrl" layout="column">
    <md-toolbar layout="row" layout-align="center center" calss="new--header">
        <div class="md-toolbar-tools">
            <img src="img/edited-logo.png" alt="Brav" class="logo new--brand-img" />
            <md-button class="new--header-icons" aria-label="Refresh Current Page" ng-if="reloadCurrentPageBtnShow" ng-click="reloadCurrentPage()">
                <md-icon md-svg-icon="icons/new/ic_refresh_white_18px.svg" id="refresh"></md-icon>
            </md-button>
            <md-button class="new--header-icons" aria-label="Full Screen" ng-if="isFullScreen" ng-click="toggleFullScreen()">
                <md-icon md-svg-icon="icons/ic_fullscreen_exit_white_48px.svg" ng-if="isFullScreen" id="ex_fscreen"></md-icon>
            </md-button>
            <md-button class="new--header-icons" aria-label="Full Screen off" ng-if="!isFullScreen" ng-click="toggleFullScreen()">
                <md-icon md-svg-icon="icons/ic_fullscreen_white_48px.svg" ng-if="!isFullScreen" id="fscreen"></md-icon>
            </md-button>
            <span flex></span>
            <md-button class="new--header-icons" aria-label="Chat" ng-if="showChatIcon" ng-click="toggleChatNav()">
                <md-icon md-svg-icon="icons/ic_chat_white_48px.svg" id="chat"></md-icon>
            </md-button>
        </div>

        <md-menu>
            <md-button aria-label="Options" class="md-icon-button" ng-click="$mdOpenMenu()">
                <md-icon md-menu-origin md-svg-icon="icons/new/ic_settings_power_white_18px.svg"></md-icon>
            </md-button>
            <md-menu-content width="4">
                <md-menu-item md-menu-align-target>
                    <md-button ng-href="#/profile">
                        My Account
                    </md-button>
                </md-menu-item>
                <md-menu-item>
                    <md-button ng-href="#/">
                        Settings
                    </md-button>
                </md-menu-item>
                <md-menu-item>
                    <md-button ng-click="logout()" ng-href="#">
                        Logout
                    </md-button>
                </md-menu-item>
            </md-menu-content>
        </md-menu>

    </md-toolbar>

    <md-content class="container" ng-controller="sidebarLinksCtrl" ng-init="loadSideBarLinks()" layout="row" flex>

        <!-- head -->
        <md-sidenav class="md-sidenav-left white" md-component-id="left" md-whiteframe="4" md-is-locked-open="true" style="width: 80px !important;">
            <md-list flex ng-mouseenter="inOrOutofNav(true)" ng-mouseleave="inOrOutofNav(false)" class="pa2">
                <md-list-item ng-click="callOpenSideNavFromParent(link)" aria-label="{{link.title}}" ng-repeat-start="link in links" ng-if="!link.disable">

                    <md-button class="new--remove-btton-style" ng-href="{{link.href}}" ng-click="openSideNav(link)">
                        <img ng-src="{{link.src}}" class="pa1 ma0" width="40" height="40" />
                        <md-tooltip md-direction="right" class="large f3 white">{{link.title}}</md-tooltip>
                    </md-button>

                </md-list-item>

                <md-list-item ng-show="outOfNav && link.open" ng-repeat-end ng-repeat="sublink in link.sub" aria-label="{{sublink.title}}">

                    <a ng-href="{{sublink.href}}">
                        <img ng-src="{{sublink.src}}" class="pa1 ma0" width="40" height="40" />
                        <md-tooltip md-direction="right" class="large f3 white">{{sublink.title}}</md-tooltip>
                    </a>
                </md-list-item>
            </md-list>

        </md-sidenav>

        <md-content flex ng-init="onload()" class="ph2 brav-bg">

            <!-- angular templating -->
            <!-- this is where content will be injected -->
            <div ng-view flex></div>

        </md-content>

    </md-content>

    </div>

</body>

